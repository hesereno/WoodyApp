<?xml version='1.0' encoding='utf-8'?>
<widget id="com.app.woody" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Woody</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <splash density="land-hdpi" src="./www/img/splashScreen.png" />
        <splash density="land-ldpi" src="./www/img/splashScreen.png" />
        <splash density="land-mdpi" src="./www/img/splashScreen.png" />
        <splash density="land-xhdpi" src="./www/img/splashScreen.png" />
        <splash density="port-hdpi" src="./www/img/splashScreen.png" />
        <splash density="port-ldpi" src="./www/img/splashScreen.png" />
        <splash density="port-mdpi" src="./www/img/splashScreen.png" />
        <splash density="port-xhdpi" src="./www/img/splashScreen.png" />
        <icon density="ldpi" src="./www/img/logo.png" />
        <icon density="mdpi" src="./www/img/logo.png" />
        <icon density="hdpi" src="./www/img/logo.png" />
        <icon density="xhdpi" src="./www/img/logo.png" />
        <icon density="xxhdpi" src="./www/img/logo.png" />
        <icon density="xxxhdpi" src="./www/img/logo.png" />
    </platform>
    <preference name="Orientation" value="portrait" />
    <preference name="SplashMaintainAspectRatio" value="false" />
    <preference name="AutoHideSplashScreen" value="false" />
</widget>